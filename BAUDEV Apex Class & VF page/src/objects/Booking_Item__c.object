IF(CONTAINS(&quot;HH7F:AA1F:AA2F:AA7F:AABF:AAEF:AAKF:AAWF:BB4F:BB7F:BBBF:BBEF:BBIF:BBKF:BBWF:E1KF:E2KF:EE2F:EE4F:EE5F:EE7F:EE8F:EECF:EEEF:EEOF:HH2F:HHBF:HHKF:HHWF:HI1F:II2F:II4F:II7F:IIBF:IIEF:IIIF:IIKF:IIWF:JJ2F:JJ4F:JJ7F:JJBF:JJEF:JJKF:JJWF:KK2F:KKAF:KKBF:KKFF:KKJF:KKKF:KKWF:OOAF :OOBF:OOFF:OOJF:OOOF:OOWF:OOZF:SL7F:SS1F:SS4F:SS7F:SSDF:SSEF:SSGF:SSKF:SSNF:SSPF:TT2F:TT7F:TTAF :TTBF:TTFF:TTJF:VV2F:VV7F:VVEF:VVKF:VVWF:X6KF:X6WF:YY2F:YYAF:YYBF:YYFF:YYJF:YYKF:YYWF:ZZ2F:ZZ7F:ZZBF:ZZEF:ZZKF:ZZWF&quot;, LEFT(Tour_Number__c,4)), &quot;APAC&quot;,

IF(CONTAINS(&quot;L1PF:L78F:GGBF:GI1F:L1AF:L1KF:L1MF:L1NF:L1PF:L1PF:L2NF:L2NF :L2PF:L74F:L76F :L77F :L9OF:LDKF:LE4F:LE7F:LE8F:LGKF:LGNF:LGPF:LI3F:LI4F:LI5F:LI6F:LL1F:LL2F:LL4F:LL5F:LL6F:LL7F:LL8F:LL9F:LLCF:LLFF:LLKF:LLLF:LLOF:LLSF:MMKF:MMOF:RRKF&quot;, LEFT(Tour_Number__c,4)), &quot;EUROPE&quot;,


IF(CONTAINS(&quot;DD2F:DD6F:DD7F:DD9F:DDDF:DDKF:WI2F:WW4F:WW5F:WW7F :WW8F:WWCF:WWKF:WWWF&quot;, LEFT(Tour_Number__c,4)), &quot;MEA&quot;,

&quot;Other&quot;)))))</formula>
